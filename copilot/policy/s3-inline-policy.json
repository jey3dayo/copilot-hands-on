{
  "Version": "2012-10-17",
  "Statement": [
    {
      "Effect": "Allow",
      "Action": [
        "s3:Put*",
        "s3:Get*",
        "s3:List*",
        "s3:Delete*"
      ],
      "Resource": [
        "arn:aws:s3:::copilot-hands-on/*",
        "arn:aws:s3:::copilot-hands-on"
      ]
    },
    {
      "Effect": "Allow",
      "Action": "s3:ListAllMyBuckets",
      "Resource": "*"
    }
  ]
}
