{
  "name": "copilot-hands-on",
  "version": "1.0.0",
  "description": "copilot hands on",
  "type": "module",
  "prisma": {
    "seed": "node --loader ts-node/esm prisma/seed.mts"
  },
  "scripts": {
    "ts": "npx ts-node",
    "ts-esm": "node --loader ts-node/esm",
    "start": "yarn build:live",
    "test": "run-s eslint",
    "build": "npx prisma generate && tsc",
    "build:live": "nodemon --watch 'src/**/*.mts' --exec 'node' --loader ts-node/esm src/index.mts",
    "deploy": "copilot deploy --env production",
    "eslint": "eslint src/**/*.mts",
    "eslint:fix": "eslint --fix src/**/*.mts"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/CyberAgent-Infosys/caad-opelake-etl-drive"
  },
  "license": "UNLICENSED",
  "devDependencies": {
    "@types/node": "^18.0.3",
    "@typescript-eslint/eslint-plugin": "^5.30.5",
    "@typescript-eslint/parser": "^5.30.5",
    "@typescript-eslint/typescript-estree": "^5.30.5",
    "cross-env": "^7.0.3",
    "eslint": "^8.19.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-airbnb-typescript": "^17.0.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-import-resolver-typescript": "^3.2.5",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jsx-a11y": "^6.6.0",
    "eslint-plugin-prettier": "^4.2.1",
    "nodemon": "^2.0.19",
    "npm-run-all": "4.1.5",
    "prettier": "^2.7.1",
    "typescript": "^4.7.4",
    "typescript-transform-paths": "^3.3.1"
  },
  "dependencies": {
    "@prisma/client": "^4.0.0",
    "node-fetch": "^3.2.8",
    "prisma": "^4.0.0",
    "prop-types": "^15.8.1",
    "ts-node": "^10.8.2"
  },
  "private": true
}
